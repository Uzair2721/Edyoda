    "class Point:\n",
    "    def __init__(self, x, y, z):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        self.z = z\n",
    "        \n",
    "    def sqSum(self):\n",
    "        result = self.x**2 + self.y**2 + self.z**2\n",
    "        return result\n",
    "\n",
    "obj = Point(1,3,5)\n",
    "obj.sqSum()"
   ]
