    "def reverse_string(str1):\n",
    "    rstr1 = ''\n",
    "    index = len(str1)\n",
    "    while index > 0:\n",
    "        rstr1 += str1[index - 1]\n",
    "        index = index - 1\n",
    "    return rstr1\n",
    "word = input(\"Enter a string: \")\n",
    "print(reverse_string(word))"
   ]
